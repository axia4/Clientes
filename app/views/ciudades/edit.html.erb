<h1>Editing ciudad</h1>

<%= render 'form' %>
<%- if ( request.xhr? ) -%>
  <%= link_to( 'Show', ciudade_path(@ciudade.id), { :onClick => "jQuery( '#edit_ciudad_form' ).dialog( 'close' );return( showUrlInDialog( '#{ciudade_path(@ciudade.id)}' ) );" } ) %> |
  <%= link_to 'Close', '#', { :onClick => "jQuery( '#edit_ciudad_form' ).dialog( 'close' );return( false );" } %>
<%- else -%>
<%= link_to 'Show', @ciudade %> |
<%= link_to 'Back', ciudades_path %>
<%- end -%>

